		"test": "razzle test --env=jsdom",
		"start:prod": "NODE_ENV=production node build/server.js",
		"lint": "eslint --ext .tsx,.ts,.js --quiet",
		"lint:fix": "eslint --ext .tsx,.ts,.js --quiet --fix",
		"type-check": "tsc --noEmit"
	},
	"main": "index.js",
	"dependencies": {
